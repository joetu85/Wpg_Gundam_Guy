<nav id="sideNav">
  <h3>Search By Keyword</h3>
  <%= form_tag search_results_path, method: :get do %>
    <%= text_field_tag :search_keywords %>
    <%= submit_tag 'Search' %>
  <% end %>
  <br />
  <h3>Shop By Category:</h3>
  <% @categories.each do |category| %>
    <%= link_to category.name, category %>
    <br />
  <% end %>
</nav>